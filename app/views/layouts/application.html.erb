<!DOCTYPE html>
<html>
<head>
  <title>GameUp</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<%= "Welcome, #{current_user.username}" if logged_in?%>
<%= link_to "Sign Out", session_url, method: :delete if logged_in?%>
<%= link_to "Sign In", new_session_url unless logged_in?%>
<%= link_to "Sign Up", new_user_url unless logged_in?%>
<%= yield %>

</body>
</html>
